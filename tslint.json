{
  "extends": "tslint:latest",
  "rules": {
    "interface-name": false,
    "object-literal-sort-keys": false,
    "semicolon": [true, "never"],
    "indent": [true, "spaces", 4],
    "trailing-comma": [
      true,
      {
        "multiline": {
          "objects": "always",
          "arrays": "always",
          "functions": "never",
          "typeLiterals": "never"
        }
      }
    ]
  }
}
